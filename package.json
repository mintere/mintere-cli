{
  "name": "mintere",
  "version": "0.3.2",
  "author": "Mintere (mintere.com)",
  "bin": {
    "mintere": "./bin/run"
  },
  "bugs": "https://github.com/mintere/mintere-cli/issues",
  "dependencies": {
    "@mintere/sites": "^2.0.2",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "chokidar": "^3.4.0",
    "form-data": "^3.0.0",
    "generator-mintere": "^0.2.0",
    "koa": "^2.12.0",
    "node-fetch": "^3.1.1",
    "tar-fs": "^2.1.0",
    "tslib": "^2.0.0",
    "yeoman-generator": "^4.10.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/koa": "^2.11.3",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-fetch": "^2.5.7",
    "@types/tar-fs": "^2.0.0",
    "chai": "^4",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mintere/mintere-cli",
  "keywords": [
    "oclif"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mintere",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "sites": {
        "description": "develop & manage Mintere sites"
      }
    }
  },
  "repository": "mintere/mintere-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
